{
  "name"        : "pg-live-select",
  "version"     : "0.0.11",
  "description" : "Live updating PostgreSQL SELECT statements",
  "main"        : "lib/LivePG.js",
  "license"     : "MIT",
  "repository"  : "https://github.com/numtel/pg-live-select.git",

  "contributors" : [
    "numtel <ben@latenightsketches.com>",
    "nothingisdead <rbmyr8@gmail.com>"
  ],
  "engines": {
